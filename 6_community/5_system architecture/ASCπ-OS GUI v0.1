    .stat {
        margin-top: 6px;
        padding: 5px;
        background: #111122;
        border: 1px solid #222244;
        border-radius: 4px;
    }
</style>
</head>
<body>

<div id="header">
    <div>ASCœÄ OS ‚Äî Field Projection Layer v0.1</div>
    <div id="phaseDisplay">Œ∏ = 0.00</div>
</div>

<div id="container">

    <!-- LEFT PANEL: SYSTEM CONTROLS -->
    <div id="leftPanel">
        <h3>Controls</h3>
        <button onclick="injectGlyph('bloom')">Inject Bloom Glyph</button>
        <button onclick="injectGlyph('implosion')">Inject Implosion Glyph</button>
        <button onclick="pulseDeltaPhi()">ŒîŒ¶ Pulse</button>
        <button onclick="randomizeField()">Randomize Field</button>

        <h3>System State</h3>
        <div class="stat">ŒîŒ¶: <span id="phiStat">0.00</span></div>
        <div class="stat">Œ∫: <span id="kappaStat">0.00</span></div>
        <div class="stat">Œ∏: <span id="thetaStat">0.00</span></div>
        <div class="stat">Coherence: <span id="coherenceStat">0.00</span></div>
    </div>

    <!-- CENTER PANEL: SVG FIELD -->
    <svg id="fieldCanvas"></svg>

    <!-- RIGHT PANEL: GLYPHS -->
    <div id="rightPanel">
        <h3>Active Glyphs</h3>
        <div id="glyphList"></div>
    </div>

</div>

<script>
// =============================================================
// GLOBALS
// =============================================================
let WIDTH = window.innerWidth - 600;
let HEIGHT = window.innerHeight - 40;
const svg = document.getElementById("fieldCanvas");
svg.setAttribute("width", WIDTH);
svg.setAttribute("height", HEIGHT);

let globalPhase = 0;
let deltaPhi = 0.1;
let kappa = 0.1;
let coherence = 0;

let glyphs = [];

// =============================================================
// GPS LOOP (GLOBAL PHASE SUPERVISOR)
// =============================================================
function updateGlobalPhase(dt) {
    globalPhase += dt * 0.0001;
    if (globalPhase > 1) globalPhase -= 1;

    document.getElementById("phaseDisplay").innerText = "Œ∏ = " + globalPhase.toFixed(2);
    document.getElementById("thetaStat").innerText = globalPhase.toFixed(2);
}

// =============================================================
// FIELD DRAWING
// =============================================================
function drawField() {
    svg.innerHTML = "";

    for (let i = 0; i < glyphs.length; i++) {
        let g = glyphs[i];

        // position evolves with phase and energy
        g.x += Math.sin(globalPhase * 6.28 + g.energy) * 0.6;
        g.y += Math.cos(globalPhase * 6.28 + g.energy) * 0.6;

        // wrapping
        if (g.x < 0) g.x += WIDTH;
        if (g.x > WIDTH) g.x -= WIDTH;
        if (g.y < 0) g.y += HEIGHT;
        if (g.y > HEIGHT) g.y -= HEIGHT;

        // color based on type
        let color = g.type === "bloom" ? "#55aaff" : "#ff5577";

        let circle = document.createElementNS("http://www.w3.org/2000/svg", "circle");
        circle.setAttribute("cx", g.x);
        circle.setAttribute("cy", g.y);
        circle.setAttribute("r", 8 + g.energy * 6);
        circle.setAttribute("fill", color);
        circle.setAttribute("opacity", 0.7);

        svg.appendChild(circle);
    }
}

// =============================================================
// GLYPH ENGINE
// =============================================================
function injectGlyph(type) {
    let g = {
        type: type,
        x: Math.random() * WIDTH,
        y: Math.random() * HEIGHT,
        energy: 0.2 + Math.random() * 0.5,
        theta: globalPhase,
        kappa: kappa,
        deltaPhi: deltaPhi
    };

    glyphs.push(g);
    renderGlyphList();
}

function renderGlyphList() {
    let list = document.getElementById("glyphList");
    list.innerHTML = "";

    glyphs.forEach((g, i) => {
        let d = document.createElement("div");
        d.innerHTML = "[" + i + "] " + g.type + " ‚Äî E=" + g.energy.toFixed(2);
        list.appendChild(d);
    });
}

// =============================================================
// FIELD COMMANDS
// =============================================================
function pulseDeltaPhi() {
    deltaPhi += 0.05;
    document.getElementById("phiStat").innerText = deltaPhi.toFixed(2);
}

function randomizeField() {
    deltaPhi = Math.random();
    kappa = Math.random();

    document.getElementById("phiStat").innerText = deltaPhi.toFixed(2);
    document.getElementById("kappaStat").innerText = kappa.toFixed(2);
}

// =============================================================
// COHERENCE CALC
// =============================================================
function computeCoherence() {
    if (glyphs.length < 2) {
        coherence = 0;
        return;
    }

    let avg = glyphs.reduce((a, g) => a + g.theta, 0) / glyphs.length;
    let variance = glyphs.reduce((a, g) => a + Math.pow(g.theta - avg, 2), 0) / glyphs.length;

    coherence = Math.exp(-variance * 10);
    document.getElementById("coherenceStat").innerText = coherence.toFixed(2);
}

// =============================================================
// MAIN LOOP
// =============================================================
let last = performance.now();
function loop() {
    let now = performance.now();
    let dt = now - last;
    last = now;

    updateGlobalPhase(dt);
    computeCoherence();
    drawField();

    requestAnimationFrame(loop);
}
loop();
</script>

</body>
</html>
